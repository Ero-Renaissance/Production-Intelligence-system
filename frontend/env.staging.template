# Staging Environment Configuration Template
# Copy to .env.staging for staging deployment
# Production Intelligence System

# API Configuration
VITE_API_BASE_URL=https://api-staging.production-intelligence.example.com

# Data Configuration
VITE_USE_MOCK_DATA=false
VITE_MOCK_WEBSOCKET_ENABLED=false

# Development Features (Limited in Staging)
VITE_DEBUG_MODE=false
VITE_SHOW_PERFORMANCE_METRICS=true

# Security (Staging)
VITE_DISABLE_AUTH=false
VITE_REQUIRE_2FA=true
VITE_SESSION_TIMEOUT_MINUTES=120

# Analytics (Staging)
VITE_ANALYTICS_ENABLED=true
VITE_ANALYTICS_ENDPOINT=https://analytics-staging.example.com

# Feature Flags (Staging Testing)
VITE_FEATURE_OPTIMIZATION_AI=true
VITE_FEATURE_CARGO_FORECAST=true
VITE_FEATURE_REAL_TIME_UPDATES=true
VITE_FEATURE_BETA_FEATURES=true

# Monitoring
VITE_ERROR_REPORTING_ENABLED=true
VITE_PERFORMANCE_MONITORING=true 